file = {:Users/thomasfung/Documents/Mendeley Desktop/Ribeiro et al. - 2013 - Annona mucosa Jacq . ( Annonaceae ) A promising source of bioactive compounds against Sitophilus zeamais Mots ..pdf:pdf},
issn = {0022474X},
journal = {Journal of Stored Products Research},
keywords = {Acetogenins,Alkaloids,Allelochemicals,Bioinsecticide,Maize weevil},
pages = {6--14},
publisher = {Elsevier Ltd},
title = {{Annona mucosa Jacq. (Annonaceae): A promising source of bioactive compounds against Sitophilus zeamais Mots. (Coleoptera: Curculionidae)}},
url = {http://dx.doi.org/10.1016/j.jspr.2013.06.001},
volume = {55},
year = {2013}
}

