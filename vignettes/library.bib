issn = {21562202},
journal = {Journal of Geophysical Research},
keywords = {Maina},
mendeley-tags = {Maina},
number = {12},
pages = {6745--6764},
title = {{The last millennium climate reanalysis project: Framework and first results}},
volume = {121},
