arxivId = {1610.08244},
author = {Chatla, Suneel Babu and Shmueli, Galit},
eprint = {1610.08244},
file = {:Users/thomasfung/Documents/Paper Collection/Chatla {\&} Shmueli (2016).pdf:pdf},
keywords = {backfitting,irls,li-shan huang and prof,over and under dispersion,prof,satish iyengar for their,smoothing splines,the authors gratefully acknowledge,time series,valuable},
